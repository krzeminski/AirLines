<%- include("partials/header") -%>
<main class="text-center">
  <form action="/" class="form-signin" method="POST">
  <img class="mb-4 logo" src="images/logo.jpg" alt="" width="72" height="72">
  <h1 class="h3 mb-3">Welcome Again!</h1>
  <input type="email" class="form-control top" name="email" placeholder="Email" required autofocus>
  <input type="password" class="form-control bottom" name="password" placeholder="Password" required>
  <button class="btn btn-lg btn-success btn-block" type="submit">Log In!</button>
</form>
</main>
<%- include("partials/footer") -%>
